{
  "compilerOptions": {
    "jsx": "react-jsx",
    "baseUrl": "src",
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "skipLibCheck": true,
    "paths": {
      "@components/*": [
        "components/*"
      ],
      "@pages/*": [
        "pages/*"
      ],
      "@utils/*": [
        "utils/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@common/*": [
        "common/*"
      ],
      "@hooks/*": [
        "hooks/*"
      ],
      "@routes/*": [
        "routes/*"
      ],
      "@store/*": [
        "store/*"
      ],
      "@layouts/*": [
        "layouts/*"
      ]
    },
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src"
  ]
  
}